# Select scheme to integrate Brownian equations
scheme                                   stochastic_adams_bashforth_rollers

# Select implementation to compute M and M*f
# We recommend to use the C++ implementation which is faster,
# see documentation
mobility_blobs_implementation            C++
mobility_vector_prod_implementation      pycuda_in_plane

# Select implementation to compute the blobs-blob interactions
blob_blob_force_implementation           python

# Set time step, number of steps and save frequency
dt                                       0.1
n_steps                                  200000
n_save                                   2
initial_step			         -100000



# Solver params
solver_tolerance                         1.0e-3


# Set fluid viscosity (eta), gravity*blob_mass (g) and blob radius
eta                                      1.0e-3
g                                        0.0 #0.072
tilt_angle				 1.047
blob_radius                              2.1


domain					 in_plane

# Stochastic parameters
kT                                       0.0041419464

# RFD parameters
rf_delta                                 1.0e-4

# Periodic dimensions
periodic_length                          0 0 0

# Set interaction with the wall
# eps = 0.5*mg
repulsion_strength_wall                  0 #0.036
debye_length_wall	   		 0.21

# Define implementation to compute the blobs-blob force and pass its arguments
repulsion_strength	   		 0.0082838928 #0.036
debye_length	   			 8.4

# Set output name
output_name                              ./two_sphere_test
save_clones				                       one_file

structure	/home/bs162/Sedimentation/RigidMultiblobsWall/multi_bodies/Structures/blob.vertex	./two_sphere.clones
